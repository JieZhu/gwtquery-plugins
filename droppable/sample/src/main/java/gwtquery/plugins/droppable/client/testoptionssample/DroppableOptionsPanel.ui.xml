<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:DecoratorPanel>
		<g:HTMLPanel>
			<table cellpadding="2" style="margin:5px;">
				<tbody>
					<tr style="vertical-align:top;">
						<td>
							<g:HTML text="Drop disabled" />
						</td>
						<td>
							<g:CheckBox ui:field="disabledCheckBox" />
						</td>
						<td>
							<g:HTML text="Disable the drop functionnality on this element" />
						</td>
					</tr>
					<tr style="vertical-align:top;">
						<td>
							<g:HTML text="Greddy options" />
						</td>
						<td>
							<g:CheckBox ui:field="greedyCheckBox" />
						</td>
						<td>
							<g:HTML text="This option prevents event propagation on nested droppables." />
						</td>
					</tr>
					<tr style="vertical-align:top;">
						<td>
							<g:HTML text="Droppable scope" />
						</td>
						<td>
							<g:TextBox ui:field="scopeBox" />
						</td>
					</tr>
					<tr style="vertical-align:top;">
						<td>
							<g:HTML text="Drop tolerance" />
						</td>
						<td>
							<g:ListBox ui:field="toleranceListBox" />
						</td>
					</tr>
					<tr style="vertical-align:top;">
						<td>
							<g:HTML text="Accept options" />
						</td>
						<td>
							<g:ListBox ui:field="acceptFunctionListBox" />
						</td>
					</tr>
					<tr style="vertical-align:top;">
						<td>
							<g:HTML text="Active class" />
						</td>
						<td>
							<g:ListBox ui:field="activeClassListBox" />
						</td>
					</tr>
					<tr style="vertical-align:top;">
						<td>
							<g:HTML text="Hover class" />
						</td>
						<td>
							<g:ListBox ui:field="hoverClassListBox" />
						</td>
					</tr>
				</tbody>
			</table>
		</g:HTMLPanel>
	</g:DecoratorPanel>
</ui:UiBinder>  